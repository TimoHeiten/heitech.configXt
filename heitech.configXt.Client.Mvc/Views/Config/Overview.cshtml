@model ConfigItems

@{
    ViewData["Title"] = "Config Items";
    Layout = "_Layout";
}

<div class="text-center">
    <table class="table table-sm table-bordered"> 
        <thead class="bg-primary text-white">
            <th>Id</th>
            <th>Name</th>
            <th>Value</th>
        </thead>
        <tbody>
            @foreach (ConfigItem item in @Model.List)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Value</td>
                </tr>
            }
        </tbody>
    </table>
</div>
